# Configurações do Aplicativo
FLASK_APP=app.py
FLASK_ENV=development
FLASK_DEBUG=1
PORT=5001

# Configurações do Supabase
SUPABASE_URL=sua_url_do_supabase
SUPABASE_KEY=sua_chave_do_supabase
SUPABASE_JWT_SECRET=seu_segredo_jwt_do_supabase

# Configurações do Clerk
CLERK_API_KEY=sua_chave_api_do_clerk
CLERK_JWT_KEY=sua_chave_jwt_do_clerk
CLERK_FRONTEND_API=sua_api_frontend_do_clerk

# Configurações de Segurança
SECRET_KEY=gere_uma_chave_secreta_forte
CORS_ORIGINS=http://localhost:5001,http://127.0.0.1:5001

# Configurações do Servidor
GUNICORN_WORKERS=4
GUNICORN_THREADS=2
GUNICORN_TIMEOUT=120
GUNICORN_BIND=0.0.0.0:5001

# Configurações de Log
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s 