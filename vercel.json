{
  "version": 2,
  "builds": [
    {
      "src": "server/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "server/app.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
