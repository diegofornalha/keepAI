{
  "version": 2,
  "builds": [
    {
      "src": "server/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/server/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server/app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_ENV": "production"
  },
  "functions": {
    "server/app.py": {
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": ["server/**"]
    }
  },
  "regions": ["gru1"]
}
